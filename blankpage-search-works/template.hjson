{
	templateId: blankpage-search-works
	templateRole: search
	extends: ["blankpage-search"]
	model: Stream
	containedBy: ["top"]
	icon: book
	label: OneBlankPage Work Search
	description: Search Engine for Works
	schema: {
		type:object
		properties: {
			label: {type:"string"}
			summary: {type:"string"}
			data: {
				type:object
				properties: {
					searchText: {type:"string"}
					content: {type:"string"}
				}
			}
		}    
	}

	actions: {
		
		view-exhibits-list: {
			roles: ["anonymous"]
			steps: [{do:"view-html"}]
		}

		feed: {
			roles: ["anonymous"]
			steps: [
				{do:"view-feed", search-types:["Artwork"]}
			]
		}
	}
}